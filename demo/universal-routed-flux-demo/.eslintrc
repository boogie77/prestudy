{
  "plugins": [
    "react"
],
  "ecmaFeatures": {
        "modules": true,
        "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules":{
      "no-console":0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
  }
}
